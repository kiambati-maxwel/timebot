{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": 0,
        "quotes": 0,
        "import/no-extraneous-dependencies": 0,
        "import/extensions": 0,
        "quote-props": 0
    }
}
